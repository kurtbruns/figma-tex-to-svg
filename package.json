{
  "name": "tex-to-svg",
  "version": "1.0.1",
  "description": "Figma plugin to render TeX as SVG and embed it in a Figma Design Document.",
  "main": "dist/code.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.config.js --mode=production",
    "dev": "NODE_ENV=development webpack serve --config webpack.config.js --mode=development",
    "watch": "NODE_ENV=development webpack --config webpack.config.js --mode=development --watch"
  },
  "author": "kurtbruns",
  "license": "MIT",
  "devDependencies": {
    "@figma/plugin-typings": "*",
    "css-loader": "^7.1.2",
    "html-inline-css-webpack-plugin": "^1.11.2",
    "html-inline-script-webpack-plugin": "^3.2.1",
    "html-webpack-plugin": "^5.6.5",
    "mini-css-extract-plugin": "^2.9.4",
    "sass": "^1.96.0",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "mathjax": "^3.2.2"
  }
}
