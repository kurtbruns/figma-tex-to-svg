{
  "name": "mathtex-editor",
  "version": "2.0.4",
  "description": "Figma plugin to render TeX as SVG and embed it in a Figma Design Document.",
  "main": "dist/code.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.config.js --mode=production",
    "dev": "concurrently \"npm run watch\" \"http-server dist -p 5173 -o\"",
    "watch": "NODE_ENV=development webpack --config webpack.config.js --mode=development --watch"
  },
  "author": "kurtbruns",
  "license": "MIT",
  "devDependencies": {
    "@figma/plugin-typings": "*",
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.2",
    "html-inline-css-webpack-plugin": "^1.11.2",
    "html-inline-script-webpack-plugin": "^3.2.1",
    "html-webpack-plugin": "^5.6.5",
    "http-server": "^14.1.1",
    "mini-css-extract-plugin": "^2.9.4",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  }
}
